# Copy this file to `.env` and fill in your secrets. NEVER commit real secrets to version control.

# Groq API key (required)
GROQ_API_KEY=your_groq_api_key_here

# Postgres database URL (required for saving reports)
# Format example: postgresql+psycopg2://username:password@host:5432/dbname
DATABASE_URL=postgresql+psycopg2://user:pass@localhost:5432/finhack_db
